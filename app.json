{
    "name": "ＱＵＥＥＮ -ＩＺＵＭＩ - V1 WhatsApp Bot",
    "description": "This is a 3ʳᵈ Party Application software for WhatsApp with Many Feature & many Commands",
    "repository": "https://github.com/vajirabot1/QUEEN-IZUMI-MD",
    "logo": "https://telegra.ph/file/ba8ea739e63bf28c30b37.jpg",
    "keywords": [
        "nodejs",
        "Bot",
        "WhatsApp Bot",
        "WhatsApp Automation",
        "Multi Device"
    ],
    "env": {
        "SESSION_ID": {
            "description": "Type the Session-ID you got from scaning QR",
            "required": true,
            "value": "dont want to put session id when you upload cred.json file"
        },
        "GIT_TOKEN": {
            "description": "Put new github token here",
            "required": true,
            "value": ""
        },
        "PREFIX": {
            "description": "Put new Prefix here",
            "required": true,
            "value": "."
        },
        "HEROKU_API_KEY": {
            "description": "Enter your heroku account api key",
            "required": true,
            "value": ""
        },
        "HEROKU_APP_NAME": {
            "description": "Enter you pushed app Name",
            "required": true,
            "value": ""
        }
    },
    "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
        },
        {
            "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
        }
    ],
    "addons": [
        {
            "plan": "heroku-postgresql",
            "options": {
                "version": "13"
            }
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "standard-2x"
        }
    }
}
